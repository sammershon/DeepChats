<h1>Create a new DeepChat</h1>

<%= form_for @chat_room do |f| %>
<div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, autofocus: true, class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.label :question %>
    <%= f.text_field :question, :value => "#{@new_question}",autofocus: true, class: 'form-control' %>
</div>

<%= f.submit "Add!", class: 'btn btn-primary' %>
<% end %>
